{
  "name": "ewloalepack",
  "displayName": "EWLoalePack",
  "description": "Eastward Locale Pack Validator",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "locale_pack",
      "aliases": ["LocalePack", "locale_pack"],
      "extensions": [".locale_pack"],
